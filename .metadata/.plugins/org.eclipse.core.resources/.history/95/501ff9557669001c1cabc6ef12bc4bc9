import java.util.Comparator;

public abstract class Sort implements Comparator<String> {

	public int Compare(Object o1, Object o2) {
		
	String s = o1.toString();
		String s1 = o2.toString();
		int l1=s.length();
		int l2=s1.length();
		if(l1<l2)
			 return +1;
		 else if(l1>l2)
			 return -1;
		 else return 0;
	}

}
